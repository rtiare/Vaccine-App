@model Patient

<form method="post">
    <div class="form-group row">
        <label for="Name" class="col-sm-2 col-form-label">Patient</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="Name" name="Name">
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Vaccineid" class="col-sm-2 col-form-label">Vaccine</label>
        <div class="col-sm-10">
            <select asp-for="Vaccineid" asp-items="ViewBag.VaccineList" class="form-control">
            </select>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Add</button>
</form>