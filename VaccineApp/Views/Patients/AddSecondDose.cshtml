@model Patient


<form method="post">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>@Model.Id</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>@Model.Name</td>
            </tr>
            <tr>
                <th>First Dose</th>
                <td>@Model.FirstDose</td>
            </tr>
        </tbody>
    </table>
    <button type="submit" class="btn btn-primary">Add Second Dose</button>

</form>