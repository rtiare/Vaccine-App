@model Vaccine

<form method="post">
    <div class="form-group row">
        <label asp-for="VaccineName" class="col-sm-2 col-form-label">Name</label>
        <div class="col-sm-10">
            <input type="text" asp-for="VaccineName" class="form-control" id="VaccineName">
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="DoseRequired" class="col-sm-2 col-form-label">Doses Required</label>
        <div class="col-sm-10">
            <select asp-for="DoseRequired" class="form-control" asp-items="ViewBag.DoseList">
            </select>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="DaysBtwDose" class="col-sm-2 col-form-label">Days Between  Doses</label>
        <div class="col-sm-10">
            <input type="text" asp-for="DaysBtwDose" class="form-control" id="DaysBtwDose">
        </div>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>